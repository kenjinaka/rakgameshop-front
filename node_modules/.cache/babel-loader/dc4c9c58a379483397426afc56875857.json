CAAA,CAAA,EAAA,IAAA,KAAA,CAAA,EAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,CAAA;AACA;AACA,OAJA;;AAKA,MAAA,KAAA,CAAA,IAAA,CAAA,cAAA,GAAA,KAAA,CAAA,UAAA,CAAA,KAAA,CAAA,IAAA,CAAA,cAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,IAAA;AACA,KATA,EASA,UAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,KAXA;AAYA,GA9BA;AA+BA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,IADA,EACA;AACA,UAAA,CAAA,GAAA,IAAA,IAAA,CAAA,IAAA,CAAA;AACA,UAAA,KAAA,GAAA,CAAA,CAAA,CAAA,QAAA,KAAA,CAAA,EAAA,QAAA,EAAA;AACA,UAAA,GAAA,GAAA,CAAA,CAAA,OAAA,GAAA,QAAA,EAAA;AACA,UAAA,IAAA,GAAA,CAAA,CAAA,WAAA,EAAA;;AACA,UAAA,KAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,QAAA,KAAA,GAAA,MAAA,KAAA;AACA;;AACA,UAAA,GAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,QAAA,GAAA,GAAA,MAAA,GAAA;AACA;;AACA,aAAA,CAAA,IAAA,EAAA,KAAA,EAAA,GAAA,EAAA,IAAA,CAAA,GAAA,CAAA;AACA,KAbA;AAcA,IAAA,QAdA,sBAcA;AACA,UAAA,UAAA,GAAA,QAAA,CAAA,cAAA,CAAA,SAAA,EAAA,KAAA,CAAA,OAAA,CAAA,gBAAA,EAAA,EAAA,CAAA;AACA,aAAA,UAAA;AACA,KAjBA;AAkBA,IAAA,UAlBA,wBAkBA;AAAA;;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,6CAAA,EAAA,KAAA,IAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,gBAAA;AACA,SAFA,MAEA;AACA,UAAA,MAAA,CAAA,YAAA,GAAA,oBAAA;AACA;AACA,OANA,EAMA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OARA;AASA;AA5BA;AA/BA,CAAA","sourcesContent":["<script>\r\n    import Header from './shared/HeaderBarAdmList'\r\n\r\n    export default {\r\n        components:{\r\n            Header\r\n        },\r\n        data(){\r\n            return {\r\n                id: this.$route.params.id.replace(\":\", \"\"),\r\n                jogo: {},\r\n                usuario: {},\r\n                message: \"\",\r\n                messageError: \"\",\r\n            }    \r\n        },\r\n        mounted(){\r\n            this.usuario = this.$root.usuario;\r\n            if(this.usuario == null || !this.usuario.admin){\r\n                return this.$router.push({ name: 'login' })\r\n            }\r\n            this.$http.get('http://localhost:5000/api/admin/buscarjogos').then(res =>{\r\n                this.jogos = res.body;\r\n                this.jogos.map(j => {   \r\n                    if(j.id == this.id){\r\n                        this.jogo = j;\r\n                    }\r\n                })\r\n                this.jogo.dataLancamento = this.formatDate(this.jogo.dataLancamento);\r\n                console.log(this.jogo);\r\n            }, res => {\r\n                console.log(res);\r\n            });\r\n        },\r\n        methods: {\r\n            formatDate(date) {\r\n                let d = new Date(date);\r\n                let month = (d.getMonth() + 1).toString();\r\n                let day = d.getDate().toString();\r\n                let year = d.getFullYear();\r\n                if (month.length < 2) {\r\n                    month = '0' + month;\r\n                }\r\n                if (day.length < 2) {\r\n                    day = '0' + day;\r\n                }\r\n                return [year, month, day].join('-');\r\n            },\r\n            gerarUrl(){\r\n                let stringNova = document.getElementById('fotoUrl').value.replace('C:\\\\fakepath\\\\', \"\");\r\n                return stringNova;\r\n            },\r\n            salvarJogo(){\r\n                this.$http.put('http://localhost:5000/api/admin/alterarjogo', this.jogo).then(res => {\r\n                    if(res.status == 200){\r\n                        this.message = \"Jogo Alterado!\";\r\n                    }else{\r\n                        this.messageError = \"Erro ao Cadastrar!\";\r\n                    }\r\n                }, res => {\r\n                    console.log(res);\r\n                })\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n\r\n<template>\r\n    <div>\r\n        <Header />\r\n        <div class=\"container mb-5\">\r\n            <div class=\"row pl-5 form-new\">\r\n                <h4>Cadastro de Produto</h4>\r\n            </div>\r\n        </div>\r\n        <div class=\"container-fluid\">\r\n            <div class=\"row justify-content-center\">\r\n                <div class=\"col-7\">\r\n                    <form class=\"form-new\">\r\n                        <div class=\"row justify-content-center mt-3\">\r\n                            <div class=\"col-3\">\r\n                                <h5>Editar Jogo</h5>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"col-12 mb-3 mt-2\">\r\n                                <label class=\"form-label\">Nome do Jogo</label>\r\n                                <input v-model=\"jogo.nome\" type=\"text\" id=\"Nome\" class=\"form-control\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"col-12 mb-3 mt-2\">\r\n                                <label class=\"form-label\">Descrição</label>\r\n                                <input v-model=\"jogo.descricao\" type=\"text\" class=\"form-control\" id=\"descricao\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"col-12 mb-3 mt-2\">\r\n                                <label class=\"form-label\">Valor</label>\r\n                                <input v-model=\"jogo.valor\" type=\"text\" class=\"form-control\" id=\"descricao\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"col-12 mb-3 mt-2\">\r\n                                <label class=\"form-label\">Categoria</label>\r\n                                <input v-model=\"jogo.categoria\" type=\"text\" class=\"form-control\" id=\"categoria\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"mb-3 mt-2\">\r\n                                <label class=\"form-label\">Classificação</label>\r\n                                <input v-model=\"jogo.classificacao\" type=\"text\" class=\"form-control\" id=\"classificacao\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"col-6 mb-3 mt-2\">\r\n                                <label class=\"form-label\">Tamanho</label>\r\n                                <input v-model=\"jogo.tamanho\" type=\"text\" class=\"form-control\" id=\"tamanho\">\r\n                            </div>\r\n                            <div class=\"col-6 mb-3 mt-2\">\r\n                                <label class=\"form-label\">Data de Lançamento</label>\r\n                                <input v-model=\"jogo.dataLancamento\" type=\"date\" class=\"form-control\" id=\"dataLancamento\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"mb-3 mt-2\">\r\n                                <label class=\"form-label\">Desenvolvedora</label>\r\n                                <input v-model=\"jogo.desenvolvedora\" type=\"text\" class=\"form-control\" id=\"desenvolvedora\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"mb-3 mt-2\">\r\n                                <button class=\"btn btn-md btt-submit\" @click=\"salvarJogo()\">Confirmar</button>\r\n                            </div>\r\n                        </div>\r\n                        <div v-if=\"message\" class=\"alert alert-success alert-dismissible fade show\" role=\"alert\">\r\n                            {{ message }}\r\n                            <button type=\"button\" @click=\"message = null\" data-dismiss=\"alert\"\r\n                                aria-label=\"Close\">\r\n                                <span aria-hidden=\"true\">&times;</span>\r\n                            </button>\r\n                        </div>\r\n                        <div v-if=\"messageError\" class=\"alert alert-danger alert-dismissible fade show\"\r\n                            role=\"alert\">\r\n                            {{ messageError }}\r\n                            <button type=\"button\" @click=\"messageError = null\" data-dismiss=\"alert\"\r\n                                aria-label=\"Close\">\r\n                                <span aria-hidden=\"true\">&times;</span>\r\n                            </button>\r\n                        </div>\r\n                    </form>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>  \r\n</template>\r\n\r\n<style>\r\n    .form-new{\r\n        color: white;\r\n    }\r\n    .btt-submit{\r\n        color: #ffff;\r\n        background-color: #340E80;\r\n    }\r\n\r\n    .btt-submit:hover{\r\n        color: #ffff;\r\n        background-color: #7e64b3;\r\n    }\r\n\r\n    #input-file{\r\n        color: #ffff;\r\n        background-color: #340E80;\r\n    }\r\n\r\n    #input-file:hover{\r\n        color: #ffff;\r\n        background-color: #7e64b3;\r\n    }\r\n\r\n    \r\n</style>"],"sourceRoot":"src/components"}]}